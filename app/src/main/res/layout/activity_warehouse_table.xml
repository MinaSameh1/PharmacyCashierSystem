<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout android:background="@drawable/pharmback1" xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".WarehouseTableActivity">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="106dp"
        android:layout_height="53dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginEnd="42dp"
        android:layout_marginRight="42dp"
        android:layout_marginBottom="25dp"
        android:fontFamily="@font/balthazar"
        android:text="Warehouse Item ID:"
        android:textColor="#04BAD8"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/floatingActionButtonWarehouseTableDel"
        app:layout_constraintEnd_toStartOf="@+id/editTextWarehouseTableItemID"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="106dp"
        android:layout_height="53dp"
        android:layout_marginStart="23dp"
        android:layout_marginLeft="23dp"
        android:layout_marginEnd="42dp"
        android:layout_marginRight="42dp"
        android:layout_marginBottom="14dp"
        android:fontFamily="@font/balthazar"
        android:text="Warehouse Address :"
        android:textColor="#04BAD8"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toStartOf="@+id/editTextWarehouseTableAddress"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="105dp"
        android:layout_height="51dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="7dp"
        android:fontFamily="@font/balthazar"
        android:text="Warehouse password :"
        android:textColor="#04BAD8"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="106dp"
        android:layout_height="56dp"
        android:layout_marginStart="23dp"
        android:layout_marginLeft="23dp"
        android:layout_marginBottom="9dp"
        android:fontFamily="@font/balthazar"
        android:text="Warehouse Name :"
        android:textColor="#04BAD8"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="104dp"
        android:layout_height="49dp"
        android:layout_marginStart="22dp"
        android:layout_marginLeft="22dp"
        android:layout_marginEnd="45dp"
        android:layout_marginRight="45dp"
        android:layout_marginBottom="7dp"
        android:fontFamily="@font/balthazar"
        android:text="Warehouse ID :"
        android:textColor="#04BAD8"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toStartOf="@+id/textViewWarehouseTableID"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editTextWarehouseTablePassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginLeft="44dp"
        android:layout_marginEnd="61dp"
        android:layout_marginRight="61dp"
        android:layout_marginBottom="16dp"
        android:ems="10"
        android:fontFamily="@font/balthazar"
        android:inputType="textPassword"
        android:textColor="#04BAD8"
        app:layout_constraintBottom_toTopOf="@+id/editTextWarehouseTableName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView4" />

    <EditText
        android:id="@+id/editTextWarehouseTableName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="61dp"
        android:layout_marginRight="61dp"
        android:layout_marginBottom="29dp"
        android:ems="10"
        android:fontFamily="@font/balthazar"
        android:inputType="textPersonName"
        android:textColor="#04BAD8"
        app:layout_constraintBottom_toTopOf="@+id/editTextWarehouseTableAddress"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/editTextWarehouseTableAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="61dp"
        android:layout_marginRight="61dp"
        android:layout_marginBottom="20dp"
        android:ems="10"
        android:fontFamily="@font/balthazar"
        android:inputType="textPersonName"
        android:textColor="#04BAD8"
        app:layout_constraintBottom_toTopOf="@+id/editTextWarehouseTableItemID"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/editTextWarehouseTableItemID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="61dp"
        android:layout_marginRight="61dp"
        android:layout_marginBottom="30dp"
        android:ems="10"
        android:fontFamily="@font/balthazar"
        android:inputType="textPersonName"
        android:textColor="#04BAD8"
        app:layout_constraintBottom_toTopOf="@+id/floatingActionButtonWarehouseTableDo"
        app:layout_constraintEnd_toEndOf="parent" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButtonWarehouseTableDel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="73dp"
        android:layout_marginLeft="73dp"
        android:layout_marginBottom="23dp"
        android:clickable="true"
        android:foreground="@drawable/pharmcrossicon"
        android:textColor="#04BAD8"
        app:backgroundTint="#04BAD8"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButtonWarehouseTableDo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="63dp"
        android:layout_marginRight="63dp"
        android:layout_marginBottom="23dp"
        android:clickable="true"
        android:foreground="@drawable/pharmrighticon"
        app:backgroundTint="#04BAD8"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerViewWarehouseTable"
        android:layout_width="404dp"
        android:layout_height="328dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginEnd="3dp"
        android:layout_marginRight="3dp"
        android:layout_marginBottom="7dp"
        android:fontFamily="@font/balthazar"
        android:textColor="#04BAD8"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewWarehouseTableID"
        android:layout_width="210dp"
        android:layout_height="43dp"
        android:layout_marginStart="45dp"
        android:layout_marginLeft="45dp"
        android:layout_marginBottom="19dp"
        android:fontFamily="@font/balthazar"
        android:text="TextView"
        android:textColor="#04BAD8"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextWarehouseTablePassword"
        app:layout_constraintStart_toEndOf="@+id/textView" />

</androidx.constraintlayout.widget.ConstraintLayout>